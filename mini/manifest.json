{
   "background": {
    "page": "background.html",
    "persistent": true
  },
   "permissions": [
    "tabs",
    "notifications",
    "cookies",
    "*://*.dirty.ru/*"
   ],
   "browser_action": {
      "default_icon": {
         "19": "images/icongray.png",
         "38": "images/icongray38.png"
      },
      "default_popup": "popup.html",
      "default_title": "Уведомления dirty.ru"
   },
   "description": "Уведомления о новых событиях, комментариях, инбоксах",
    "icons": {
      "128": "images/128.png",
      "16": "images/16.png",
      "48": "images/48.png"
   },   
   "manifest_version": 2,
   "options_page": "options.html",
   "name": "Уведомления dirty.ru",
   "author": "LazyKarlson",   
   "version": "0.9"
}